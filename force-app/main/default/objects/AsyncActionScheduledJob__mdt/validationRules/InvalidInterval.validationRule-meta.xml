<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>InvalidInterval</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
    ISBLANK(ScheduledJobInterval__c),
    ScheduledJobInterval__c &lt; 1,
    ScheduledJobInterval__c &gt; 60
)</errorConditionFormula>
    <errorDisplayField>ScheduledJobInterval__c</errorDisplayField>
    <errorMessage>Enter a value between 1 and 60.</errorMessage>
</ValidationRule>
